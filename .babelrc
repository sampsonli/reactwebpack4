{
  "presets": [
    [
      "@babel/env",
      {
        "useBuiltIns": "usage",
        "targets": {
          "browsers": [
            "ie >= 9",
            "Chrome >= 21",
            "Firefox >= 1",
            "Edge >= 13",
            "last 3 versions"
          ]
        }
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-class-properties",
    ["@babel/plugin-proposal-decorators", {
      "legacy": true
    }]
  ]
}
